[run]
source = semantic_kernel
omit = 
    semantic_kernel/connectors/memory/*
    semantic_kernel/reliability/*
    semantic_kernel/memory/*

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Do not complain about missing debug-only code:
    def __repr__

    # Do not complain if tests do not hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # TYPE_CHECKING and @overload blocks are never executed during pytest run
    if TYPE_CHECKING:
    @overload
    @abstractmethod